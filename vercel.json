{
  "version": 2,
  "builds": [
    { 
      "src": "cdn.js", 
      "use": "@vercel/node" 
    },
    {
      "src": "cdn/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [ 
    { 
      "src": "/(.*)",
      "dest": "/cdn.js",
      "headers": {
        "x-vercel-max-duration": "30"
      }
    },
    {
      "src": "/(.+)",
      "dest": "cdn/$1",
      "headers": {
        "x-vercel-max-duration": "30"
      }
    }
  ]
}

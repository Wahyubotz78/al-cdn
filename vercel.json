{
  "version": 2,
  "builds": [
    { 
      "src": "cdn.js", 
      "use": "@vercel/node" 
    },
    {
        "src": "public/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [ 
    { 
        "src": "/(.*)",
        "dest": "/cdn.js"
    },
    {
        "src": "/(.+)",
        "dest": "cdn/$1"
    }
  ],
  "functions": {
    "cdn.js": {
      "maxDuration": 30
    }
}

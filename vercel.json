{
  "version": 2,
  "builds": [
    { "src": "cdn.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" }, // Menyajikan file statis
    { "src": "/upload", "dest": "/cdn.js" }, // Menangani POST /upload
    { "src": "/(.*)", "dest": "/cdn.js" } // Fallback untuk permintaan lainnya
  ]
}
